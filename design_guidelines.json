{
  "project_name": "F3 Fitness Gym",
  "design_philosophy": {
    "identity": "The Industrial Athlete",
    "core_values": [
      "High Performance",
      "Precision",
      "Intensity",
      "Modernity"
    ],
    "mood": "Dark, moody, electric, premium. Think 'Nike Pro' meets 'Cyberpunk Gym'.",
    "theme_mode": "dark"
  },
  "typography": {
    "font_family": {
      "heading": "Barlow Condensed, sans-serif",
      "body": "Manrope, sans-serif",
      "mono": "JetBrains Mono, monospace"
    },
    "scale": {
      "h1": "text-5xl md:text-7xl font-black tracking-tighter uppercase",
      "h2": "text-4xl md:text-5xl font-bold tracking-tight uppercase",
      "h3": "text-2xl md:text-3xl font-bold tracking-tight",
      "h4": "text-xl font-semibold tracking-wide uppercase",
      "body_lg": "text-lg font-medium leading-relaxed",
      "body_base": "text-base font-normal leading-relaxed text-zinc-400",
      "caption": "text-sm font-medium tracking-wider uppercase text-zinc-500"
    },
    "imports": [
      "@import url('https://fonts.googleapis.com/css2?family=Barlow+Condensed:wght@400;600;700;900&family=Manrope:wght@300;400;500;600;700&display=swap');"
    ]
  },
  "colors": {
    "palette": {
      "background": "#09090b",
      "foreground": "#fafafa",
      "card": "#18181b",
      "card_foreground": "#fafafa",
      "popover": "#09090b",
      "popover_foreground": "#fafafa",
      "primary": "#06b6d4",
      "primary_foreground": "#000000",
      "secondary": "#f97316",
      "secondary_foreground": "#ffffff",
      "muted": "#27272a",
      "muted_foreground": "#a1a1aa",
      "accent": "#27272a",
      "accent_foreground": "#fafafa",
      "destructive": "#ef4444",
      "destructive_foreground": "#fafafa",
      "border": "#27272a",
      "input": "#27272a",
      "ring": "#06b6d4"
    },
    "gradients": {
      "hero_glow": "bg-gradient-to-tr from-cyan-500/20 via-transparent to-orange-500/20",
      "card_surface": "bg-gradient-to-b from-zinc-800/50 to-zinc-900/50",
      "active_border": "bg-gradient-to-r from-cyan-500 to-blue-600"
    }
  },
  "layout": {
    "container": "max-w-7xl mx-auto px-4 sm:px-6 lg:px-8",
    "spacing": {
      "section_gap": "py-24",
      "element_gap": "gap-6"
    },
    "grid_system": {
      "dashboard_bento": "grid grid-cols-1 md:grid-cols-4 lg:grid-cols-6 gap-6",
      "marketing_hero": "grid grid-cols-1 lg:grid-cols-12 gap-12 items-center"
    },
    "border_radius": {
      "card": "rounded-xl",
      "button": "rounded-none",
      "input": "rounded-md"
    }
  },
  "components": {
    "buttons": {
      "primary": "bg-primary text-primary-foreground hover:bg-cyan-400 font-bold uppercase tracking-wider px-8 py-3 rounded-none transition-all duration-300 hover:shadow-[0_0_20px_rgba(6,182,212,0.5)]",
      "secondary": "bg-transparent border border-zinc-700 text-white hover:border-white hover:bg-white/5 font-semibold uppercase tracking-wider px-8 py-3 rounded-none transition-all duration-300",
      "ghost": "hover:bg-zinc-800 text-zinc-400 hover:text-white"
    },
    "cards": {
      "base": "bg-card/40 backdrop-blur-md border border-white/5 rounded-xl p-6 hover:border-white/10 transition-colors duration-300",
      "glass": "bg-black/40 backdrop-blur-xl border border-white/10 rounded-xl",
      "highlight": "bg-gradient-to-br from-zinc-900 to-black border border-cyan-500/30 shadow-[0_0_30px_-10px_rgba(6,182,212,0.2)]"
    },
    "inputs": {
      "base": "bg-zinc-900/50 border-zinc-800 focus:border-cyan-500 focus:ring-1 focus:ring-cyan-500 transition-all duration-300 rounded-md placeholder:text-zinc-600"
    }
  },
