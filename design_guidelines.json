    "icons": "Use 'lucide-react' for all icons."
  },
  "motion": {
    "page_transition": {
      "initial": { "opacity": 0, "y": 20 },
      "animate": { "opacity": 1, "y": 0 },
      "exit": { "opacity": 0, "y": -20 },
      "transition": { "duration": 0.4, "ease": "easeOut" }
    },
    "hover_lift": "hover:-translate-y-1 transition-transform duration-300",
    "stagger_list": "staggerChildren: 0.1"
  },
  "accessibility": {
    "contrast": "Ensure all text on dark backgrounds is at least zinc-400 (4.5:1 ratio).",
    "focus_states": "focus:ring-2 focus:ring-cyan-500 focus:outline-none"
  },
  "universal_guidelines": [
    "Do NOT use gradients for text.",
    "Do NOT use rounded-full buttons unless it is an icon button.",
    "Inputs should have a subtle background, not just a border.",
    "Use uppercase for labels and small headers to add structure.",
    "Always use 'Manrope' for numbers (prices, stats, dates).",
    "Dashboard cards should have a subtle 1px border of white/10.",
    "Use data-testid attributes for all interactive elements."
  ],
  "instructions_to_main_agent": "Implement the 'Dark Industrial' theme strictly. Use the provided colors and fonts. Do not revert to default Tailwind colors. Ensure all charts in the dashboard use the cyan/orange/white palette. Use the provided image URLs for the respective sections."
}